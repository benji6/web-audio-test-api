{
  "name": "web-audio-test-api",
  "description": "Web Audio API test library for CI",
  "version": "0.2.1",
  "author": "Nao Yonamine <mohayonao@gmail.com>",
  "bugs": {
    "url": "https://github.com/mohayonao/web-audio-test-api/issues"
  },
  "devDependencies": {
    "babel": "^5.1.13",
    "babel-eslint": "^3.0.1",
    "browserify": "^9.0.8",
    "coveralls": "^2.11.2",
    "eslint": "^0.20.0",
    "espower-babel": "^2.1.0",
    "isparta": "^3.0.3",
    "lodash": "^3.7.0",
    "mocha": "^2.2.4",
    "power-assert": "^0.11.0",
    "sinon": "^1.14.1"
  },
  "homepage": "https://github.com/mohayonao/web-audio-test-api/",
  "keywords": [
    "webaudioapi",
    "test"
  ],
  "license": "MIT",
  "main": "src/",
  "repository": {
    "type": "git",
    "url": "http://github.com/mohayonao/web-audio-test-api.git"
  },
  "scripts": {
    "build": "npm run build-browser",
    "build-browser": "browserify src -o build/web-audio-test-api.js",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha",
    "lint": "jshint --reporter node_modules/jshint-stylish/stylish.js src test",
    "test": "mocha",
    "travis": "npm run lint && npm run cover"
  }
}
